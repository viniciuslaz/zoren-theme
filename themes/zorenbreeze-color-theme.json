  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#6a7684",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["string"],
      "settings": {
        "foreground": "#94c991"
      }
    },
    {
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#e0b95a"
      }
    },
    {
      "scope": ["constant.language", "constant.character", "constant.other"],
      "settings": {
        "foreground": "#d89b78"
      }
    },
    {
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#b884d6"
      }
    },
    {
      "scope": ["keyword.control"],
      "settings": {
        "foreground": "#d896c4",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#8faec7"
      }
    },
    {
      "scope": ["entity.name.function", "support.function", "meta.function-call"],
      "settings": {
        "foreground": "#6eb4f7"
      }
    },
    {
      "scope": ["entity.name.type", "entity.name.class", "support.type", "support.class"],
      "settings": {
        "foreground": "#5ac5c5"
      }
    },
    {
      "scope": ["variable", "support.variable"],
      "settings": {
        "foreground": "#d4d7dd"
      }
    },
    {
      "scope": ["variable.other.property", "support.type.property-name"],
      "settings": {
        "foreground": "#b8d4e8"
      }
    },
    {
      "scope": ["variable.parameter"],
      "settings": {
        "foreground": "#d89b78",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["entity.name.tag"],
      "settings": {
        "foreground": "#e07878"
      }
    },
    {
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#e0b95a"
      }
    },

    {
      "scope": ["keyword.control.import", "keyword.control.from"],
      "settings": {
        "foreground": "#b884d6"
      }
    },
    {
      "scope": ["markup.heading"],
      "settings": {
        "foreground": "#6eb4f7",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.bold"],
      "settings": {
        "foreground": "#d89b78",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.italic"],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["markup.inline.raw"],
      "settings": {
        "foreground": "#94c991"
      }
    },
    {
      "scope": ["punctuation"],
      "settings": {
        "foreground": "#8f95a3"
      }
    },
    {
      "scope": ["invalid"],
      "settings": {
        "foreground": "#f09070"
      }
    },
    {
      "scope": ["invalid.deprecated"],
      "settings": {
        "foreground": "#d89b78"
      }
    }
  ]
}
